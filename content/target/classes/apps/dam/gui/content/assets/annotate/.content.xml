<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="AEM Assets | Annotate Asset"
        sling:resourceType="granite/ui/components/foundation/page"
        sling:vanityOrder="{Long}291"
        sling:vanityPath="/annotate">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                categories="[cq.gui.annotation,cq.gui.damadmin.admin,dam.gui.admin.timeline]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/page/body">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/layouts/page"/>
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/navcolumns"
                currentId="cq-assets"
                srcPath="cq/core/content/nav"/>
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/gui/components/admin/endor/breadcrumbs"
                baseUrl="/assets.html"
                contentPath="${requestPathInfo.suffix}"
                rootPath="/content/dam"
                rootTitle="Assets"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/gui/components/admin/metadataeditor/endor/title"
                title="Annotate Asset"/>
            <header
                jcr:primaryType="nt:unstructured"
                srcPath="/mnt/overlay/cq/core/content/header"/>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/endor/layouts/content"/>
                <aside
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/endor/layouts/sidepanels"/>
                    <items jcr:primaryType="nt:unstructured">
                        <search
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/include"
                            id="aem-assets-rail-search"
                            path="/mnt/overlay/dam/gui/content/commons/sidepanels/search"/>
                        <timeline
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/include"
                            id="aem-assets-rail-timeline"
                            path="/mnt/overlay/dam/gui/content/commons/sidepanels/timeline"/>
                        <redirecttopreviouspage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/components/admin/commons/redirecttopreviouspage"/>
                    </items>
                </aside>
                <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/mode"
                        group="aem-assets-admin-properties"
                        mode="${requestPathInfo.selectors[0] == &quot;edit&quot; ? &quot;edit&quot; : &quot;default&quot;}"/>
                    <items jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/navigation">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/endor/layouts/actionbar"/>
                            <items jcr:primaryType="nt:unstructured">
                                <timeline
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/innerrailtoggle"
                                    class="cq-common-admin-timeline-activator cq-common-admin-navigationpanel-changemode"
                                    foundation-mode-group="cq-damadmin-admin-childpages"
                                    foundation-mode-value="selection"
                                    hideText="{Boolean}true"
                                    icon="icon-clock"
                                    targetId="aem-assets-rail-timeline"
                                    text="Timeline"
                                    title="Timeline"/>
                                <playvideo
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    hideText="{Boolean}true"
                                    icon="icon-play-circle"
                                    rel="cq-damadmin-admin-actions-playvideo-activator"
                                    text="Play/Pause video"
                                    title="Play/Pause video"
                                    toggle="modal"/>
                                <jumptofield
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    id="jumptotime"
                                    name=":name"
                                    text="0"
                                    title="Enter Time (in seconds)"/>
                                <jump
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/submit"
                                    class="button primary jumpbtn"
                                    formId="assetpropertiesform"
                                    rel="foundation-content-control cq-damadmin-jumpto-video-time"
                                    text="Jump"/>
                                <done
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/button"
                                    class="redirect-to-previous-page"
                                    icon="icon-check"
                                    rel="cq-damadmin-asset-annotation-save"
                                    text="Done"
                                    title="Done">
                                    <layoutConfig
                                        jcr:primaryType="nt:unstructured"
                                        align="right"/>
                                    <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        formId="assetpropertiesform"
                                        foundation-content-control-action="back"
                                        foundation-content-control-refresh="{Boolean}true"
                                        foundation-mode-group="cq-siteadmin-admin-bulkproperties"
                                        foundation-mode-value="default"/>
                                </done>
                            </items>
                        </default>
                    </items>
                </header>
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <content
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/components/admin/annotation"
                            class="asset-annotation foundation-collection-container content-container"
                            output-replace="#content">
                            <renditionpicker
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="dam/gui/components/admin/renditionpicker"
                                class="target"
                                id="asset-mainimage"
                                width="{Long}1280"/>
                            <videopicker
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="dam/gui/components/admin/videopicker"
                                class="asset-detail-views-video"
                                id="video-asset"/>
                        </content>
                        <comment
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/gui/components/admin/annotation/comment"
                            output-replace="#content"/>
                    </items>
                </content>
            </content>
        </body>
    </jcr:content>
</jcr:root>
